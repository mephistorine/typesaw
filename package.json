  },
  "homepage": "https://github.com/mephistorine/typesaw#readme",
  "dependencies": {
    "case-it": "^1.0.1",
    "commander": "^7.2.0",
    "consola": "^2.15.3"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=7.10.1"
  }
